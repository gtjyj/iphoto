    {
        "key":"REQUEST_LIMIT_INTERVAL",
        "label":"请求限频区间（分钟）",
        "type":"text",
        "value":"1",
        "placeholder":"单IP地址n分钟内请求x次，则限制访问",
        "require":true
    },
    {
        "key":"REQUEST_LIMIT",
        "label":"限频次数",
        "type":"text",
        "value":"200",
        "placeholder":"单IP地址n分钟内请求x次，则限制访问",
        "require":true
