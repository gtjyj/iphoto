            {
                "key":"local",
                "label":"本地磁盘目录"
            },
            {
                "key":"tencentcos",
                "label":"腾讯云COS"
            },
            {
                "key":"aliyuncos",
                "label":"阿里云COS"
            },
            {
                "key":"webdav",
                "label":"WebDav"
            }
        ]
    },
    {
